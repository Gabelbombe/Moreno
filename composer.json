{
  "name": "ehime/moreno",
  "description": "Tracking API script",
  "keywords":    ["api", "tracker"],
  "version":     "1.0.0",
  "type":        "library",
  "license":     "MIT",
  "authors": [
    {
      "name": "Jd Daniel",
      "email": "dodomeki@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "laravel/lumen-framework": "5.1.*",
    "php":                     ">=5.6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "4.3.5",
    "mockery/mockery": "0.8.*"
  },
  "autoload": {
    "psr-0": {
      "Model":      "src/",
      "Controller": "src/",
      "View":       "src/",
      "Helper":     "src/"
    },
    "autoload-dev": {
      "classmap": [
        "tests/"
      ]
    },
    "config": {
      "preferred-install": "dist"
    }
  }
}
